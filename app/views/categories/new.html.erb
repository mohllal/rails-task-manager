<% content_for(:title, 'Create Category') %>

<h1>Create Category</h1>

<%= link_to 'Back To Categories', categories_path, class: 'btn btn-secondary mb-3' %>

<%= form_for @category, class: 'form' do |f| %>
  <%= render partial: 'form', locals: {f: f} %>

  <div class="form-group mb-2">
    <%= f.submit 'Create Category', class: 'btn btn-success' %>
  </div>
<% end %>